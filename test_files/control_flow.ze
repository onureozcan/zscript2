print("prime numbers test")

var fnc_isPrime = fun(num: int) : boolean {
    for (var i = 2; i < num; i = i + 1) {
        if (num % i == 0) {
            return false
        }
    }
    return true
}

var isPrimeFncBasedTest = false

if (isPrimeFncBasedTest) {
    for (var num = 2; num < 100; num = num + 1) {
        var isPrime = fnc_isPrime(num)
        if (isPrime) {
            print(num)
        }
    }
} else {
    for (var num = 2; num < 100000; num = num + 1) {
        var isPrime = true
        for (var i = 2; i < num; i = i + 1) {
            if (num % i == 0) {
                isPrime = false
                break
            }
        }
        if (isPrime) {
            print(num)
        }
    }
}