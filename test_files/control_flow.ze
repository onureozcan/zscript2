print("odd-even test")
for (var i = 0; true; i = i+1) {
    if (i < 5) {
        continue;
    }
    print(i)
    if (i % 2 == 0) {
        print("even")
    } else {
        print("odd")
    }
    if (i > 99) {
        break
    }
}